%==============================================================================
% Extends base template
%==============================================================================
((*- extends '


nbprint.tex.j2' -*))


%==============================================================================
% Cell
%==============================================================================
((* block any_cell scoped *))
((* if "nbprint_ignore" in cell.metadata.get("tags", []) *))
% Do Nothing if nbprint_ignore tag is set
((* elif "nbprint_lhead" in cell.metadata.get("tags", []) *))
% Ignore headers and footers
((* elif "nbprint_rhead" in cell.metadata.get("tags", []) *))
% Ignore headers and footers
((* elif "nbprint_chead" in cell.metadata.get("tags", []) *))
% Ignore headers and footers
((* elif "nbprint_lfoot" in cell.metadata.get("tags", []) *))
% Ignore headers and footers
((* elif "nbprint_rfoot" in cell.metadata.get("tags", []) *))
% Ignore headers and footers
((* elif "nbprint_cfoot" in cell.metadata.get("tags", []) *))
% Ignore headers and footers
((* elif "nbprint_title" in cell.metadata.get("tags", []) *))
% Title page
% Don't include header/footer, and vertically center + logo
% TKP helper
% \layout
\thispagestyle{empty}
\setlength{\voffset}{-0.75in}
\setlength{\headsep}{5pt}
((( draw_cell_custom(cell, draw_figure_fullwidth) )))
\newpage
\setlength{\voffset}{0.5in}
((* else *))
((* if cell.outputs *))
\pagestyle{fancy}
((( draw_cell_custom(cell, draw_figure_asis) )))
((* endif *))
((* endif *))
((* endblock any_cell *))
